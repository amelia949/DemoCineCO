        id        = "com.phonegap.example"
        versionCode="10" 
        version   = "2.9.0">

    <!-- versionCode is optional and Android only -->

    <name>Cine Colombia</name>

    <description>
        Demo de cine Colombia
    </description>

    <author href="https://amellali.itzel.com" email="amelia_949@hotmail.com">
        Amellali Lopez Garcia
    </author>

    <preference name="prerendered-icon" value="true" />

    <icon src="assets/iconos/icon-57.png" gap:platform="ios" width="57" height="57" />
